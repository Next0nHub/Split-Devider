<h1>Entry</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class="">
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.name %></td>
        <td><%= entry.description %></td>
        <td><%= entry.price %></td>
        <td><%= link_to 'Show', user_group_entry_path(entry.user,entry.group,entry) ,class: "btn btn-l btn-success"%></td>
        <td><%= link_to 'Edit', edit_user_group_entry_path(entry.user,entry.group,entry),class: "btn btn-l btn-primary" %></td>
        <td><%= link_to 'Destroy', user_group_entry_path(entry.user,entry.group,entry), method: :delete, data: { confirm: 'Are you sure?' },class: "btn btn-l btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Entry', new_user_group_entry_path(entry.user,entry.group) %>